{
    "name": "pauci/cqrs",
    "description": "CQRS library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "ramsey/uuid": "~3.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "ext-redis": "*",
        "doctrine/orm": "~2.4",
        "phpunit/phpunit": "~4.0",
        "jms/serializer": "0.*"
    },
    "suggest": {
        "ramsey/uuid-doctrine": "Provides the ability to use ramsey/uuid as a Doctrine field type"
    },
    "autoload": {
        "psr-4": {
            "CQRS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CQRSTest\\": "tests/CQRSTest/"
        }
    },
    "extras": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
