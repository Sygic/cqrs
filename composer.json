{
    "name": "pauci/cqrs",
    "description": "CQRS library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ramsey/uuid": "^4.0",
        "psr/container": "^1.0",
        "pauci/datetime": "^0.3.3"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "phpunit/phpunit": "^9.1",
        "squizlabs/php_codesniffer": "^3.5",
        "guzzlehttp/guzzle": "^6.2"
    },
    "suggest": {
        "ramsey/uuid-doctrine": "Provides the ability to use ramsey/uuid as a Doctrine field type"
    },
    "autoload": {
        "psr-4": {
            "CQRS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CQRSTest\\": "tests/CQRSTest/"
        }
    },
    "extras": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
