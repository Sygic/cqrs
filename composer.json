{
    "name": "pauci/cqrs",
    "description": "CQRS library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^5.6 || ^7.0",
        "ramsey/uuid": "^3.0.0",
        "psr/log": "^1.0.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.4.8",
        "phpunit/phpunit": "^5.1.4",
        "jms/serializer": "^1.1.0"
    },
    "suggest": {
        "ramsey/uuid-doctrine": "Provides the ability to use ramsey/uuid as a Doctrine field type"
    },
    "autoload": {
        "psr-4": {
            "CQRS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CQRSTest\\": "tests/CQRSTest/"
        }
    },
    "extras": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
